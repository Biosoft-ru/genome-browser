/*
 * Toggle UI action
 */
{
    id: "toggle_ui",
    label: "Toggle UI mode",
    icon: "appInfo.iconsPath+'toggleui.png'",
    visible: "function(doc)
    {
        if( perspective && perspective.name && 'Genome Enhancer' === perspective.name )
        {
            return -1;
        }
        return true;
    }",
    action: "function(event)
    {
        toggleUI();
    }"
}