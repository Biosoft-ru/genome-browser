/*
 * Undo action
 */
{
    id: "undo_in",
    label: "Undo",
    icon: "appInfo.iconsPath+'undo.gif'",
    visible: "function(doc)
    {
    	if(doc && doc.undo) 
        {    
            return !doc.readOnly;
        }
        return -1; 
    }",
    action: "function(event)
    {
        var activeDocument = getActiveDocument();
        if (activeDocument && activeDocument.undo) 
        {
            activeDocument.undo();
        }
    }"
}