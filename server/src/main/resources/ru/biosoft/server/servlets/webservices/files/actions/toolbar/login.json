/*
 * Login action for Anonymous user
 */
{
    id: "login",
    label: "Log-in",
    icon: "appInfo.iconsPath+'login.png'",
    visible: "function(doc)
    {
        if(isAnonymousSession)
           return true;
        return -1;
        
    }",
    action: "function(event)
    {
        var user = new User();
        user.logout(false);    
    }"
}