/*
 * Redo action
 */
{
    id: "redo_in",
    label: "Redo",
    icon: "appInfo.iconsPath+'redo.gif'",
    visible: "function(doc)
    {
    	if(doc && doc.redo) 
        {    
            return !doc.readOnly;
        }
        return -1; 
    }",
    action: "function(event)
    {
        var activeDocument = getActiveDocument();
        if (activeDocument && activeDocument.redo) 
        {
            activeDocument.redo();
        }
    }"
}